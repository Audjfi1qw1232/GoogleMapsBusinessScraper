# Scraping Configuration
HEADLESS=true
DEFAULT_WORKERS=3
TIMEOUT=30000
MAX_RETRIES=3

# Rate Limiting
MIN_DELAY=2000
MAX_DELAY=8000
RATE_LIMIT_PAUSE=60000

# Storage
EXPORT_PATH=./exports
BUFFER_SIZE=10
BACKUP_ENABLED=true

# Google Maps API (Optional - for hybrid approach)
GOOGLE_MAPS_API_KEY=
USE_API_FALLBACK=false

# Proxy Configuration (Optional)
PROXY_URL=
PROXY_USERNAME=
PROXY_PASSWORD=
ROTATE_PROXIES=false

# Anti-Detection
STEALTH_MODE=true
RANDOMIZE_VIEWPORT=true
RANDOMIZE_USER_AGENT=true
SIMULATE_HUMAN_BEHAVIOR=true

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs
ENABLE_SCREENSHOTS=true